        "enable": {
            "__name": "OPENSEA_ENABLE",
            "__format": "json"
        },
        "api_key": "OPENSEA_API_KEY"
    },
    "webpurify": {
        "enable": {
            "__name": "WEBPURIFY_ENABLE",
            "__format": "json"
        },
        "url": "WEBPURIFY_URL",
        "api_key": "WEBPURIFY_API_KEY"
    },
    "emailVerification": {
        "sourceEmail": "EMAIL_VERIFICATION_SOURCE_EMAIL",
        "enable": "EMAIL_VERIFICATION_ENABLE"
    }
