{
    "mongodb": {
        "url": "MONGODB_URL"
    },
    "redis": {
        "url": "REDIS_URL"
    },
    "api": {
        "access_control": {
            "__name": "CORS_ALLOWED_ORIGINS",
            "__format": "json"
        }
    },
    "web3": {
        "networkId": "DEFAULT_ETHEREUM_NETWORK_ID"
    },
    "session": {
        "secret": "AUTHENTICATION_SESSION_SECRET"
    },
    "dapper": {
        "url": "DAPPER_HTTP_PROVIDER"
    },
